{
	"name": "C++",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "ubuntu-20.04"
		}
	},
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined",
		"--env=DISPLAY",
		"--net=host",
		"--privileged"
	],
	"settings": {},
	"extensions": ["ms-vscode.cpptools", "ms-vscode.cmake-tools", "twxs.cmake"],
	"remoteUser": "vscode"
}
